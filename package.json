    "url": "https://github.com/Nain08/E-Commerce/issues"
  },
  "homepage": "https://github.com/Nain08/E-Commerce#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "express": "^4.19.2",
    "mongoose": "^8.3.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "nodemon": "^3.1.0"
  }
}
